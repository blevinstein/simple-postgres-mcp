{
  "mcpServers": {
    "simple-postgres-mcp": {
      "command": "node",
      "args": [
        "./src/index.js",
        "--host", "localhost",
        "--port", "5432",
        "--database", "mcp_memories",
        "--collection", "test_collection",
        "--embedding-model", "google/text-embedding-004"
      ],
      "env": {
        "GEMINI_API_KEY": "your-gemini-api-key",
        "PGUSER": "postgres",
        "PGPASSWORD": "postgres"
      }
    },
    "simple-postgres-mcp-multi": {
      "command": "node",
      "args": [
        "./src/index.js",
        "--host", "localhost",
        "--port", "5432",
        "--database", "mcp_memories",
        "--embedding-model", "google/text-embedding-004"
      ],
      "env": {
        "GEMINI_API_KEY": "your-gemini-api-key",
        "PGUSER": "postgres",
        "PGPASSWORD": "postgres"
      }
    }
  }
}
